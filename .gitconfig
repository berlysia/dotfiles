[include]
	path = ~/.gitconfig_local

[core]
	excludesfile = ~/.gitignore_global
	editor = code --wait
[credential]
	helper = osxkeychain
[rebase]
	autosquash = true
[merge]
	conflictstyle = diff3
	ff = false
[pull]
	ff = only
	rebase = merges
[user]
	name = berlysia
	email = berlysia@gmail.com
[init]
	defaultBranch = master
[push]
	autoSetupRemote = true
