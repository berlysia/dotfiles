{
  "mcpServers": {
    "readability": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "@mizchi/readability@latest",
        "--mcp"
      ],
      "env": {}
    },
    "playwright": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "@playwright/mcp@latest"
      ],
      "env": {}
    },
    "context7": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "@upstash/context7-mcp@latest"
      ],
      "env": {}
    },
    "o3-search-mcp": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "o3-search-mcp@latest"
      ],
      "env": {
        "OPENAI_MODEL": "gpt-5",
        "OPENAI_API_KEY": "${USER_OPENAI_API_KEY_FOR_MCP}"
      }
    },
    "vibe_kanban": {
      "args": [
        "-y",
        "vibe-kanban",
        "--mcp"
      ],
      "command": "npx"
    }
  },
  "preferredNotifChannel": "terminal_bell",
  "defaultMode": "allowEdits"
}