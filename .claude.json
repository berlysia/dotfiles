{
  "mcpServers": {
    "readability": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "@mizchi/readability@0.7.7",
        "--mcp"
      ],
      "env": {}
    },
    "chrome-devtools": {
      "command": "npx",
      "args": [
        "-y",
        "chrome-devtools-mcp@0.6.0"
      ]
    },
    "playwright": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "@playwright/mcp@0.0.41"
      ],
      "env": {}
    },
    "context7": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "@upstash/context7-mcp@1.0.20"
      ],
      "env": {}
    },
    "o3-search-mcp": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "o3-search-mcp@0.0.9"
      ],
      "env": {
        "OPENAI_MODEL": "gpt-5",
        "OPENAI_API_KEY": "${USER_OPENAI_API_KEY_FOR_MCP}"
      }
    }
  },
  "preferredNotifChannel": "terminal_bell",
  "defaultMode": "allowEdits"
}
